{
  "name": "shapediver/geometry-api-v2",
  "version": "0.1.0",
  "description": "SDK to communicate with the ShapeDiver Geometry API version 2.",
  "keywords": [
    "shapediver",
    "sdk",
    "geometry api"
  ],
  "homepage": "https://github.com/shapediver/GeometryBackendSdkPhp#readme",
  "license": "ISC",
  "authors": [
    {
      "name": "ShapeDiver",
      "homepage": "https://shapediver.com/"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "codeception/assert-throws": "^1.4",
    "phpunit/phpunit": " ^11.0",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "ShapeDiver\\GeometryApiV2\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ShapeDiver\\GeometryApiV2\\Test\\": "test/"
    }
  }
}
